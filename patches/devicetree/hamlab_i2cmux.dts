/dts-v1/;
/plugin/;
/{		
	fragment@70 {
		target-path = "/amba/i2c\@e0004000";
		__overlay__ {
			i2c-mux@70 {
				compatible = "nxp,pca9547";
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0x70>;
				status="okay";
				i2c@1 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <1>;
				};
				i2c@2 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <2>;
				};
				i2c@3 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <3>;
				};
				i2c@4 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <4>;
				};
				i2c@5 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <5>;
				};
				i2c@6 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <6>;
				};
				i2c@7 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <7>;
				};
				i2c@8 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <8>;
				};
				i2c@9 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <9>;
				};	
			};
		};
	};
};
